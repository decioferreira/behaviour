<%= form_tag account_edit_path do %>
  <%= label_tag :email %>
  <%= text_field_tag :email %>
  <%= label_tag :password %>
  <%= password_field_tag :password %>
  <%= label_tag :confirm_password %>
  <%= password_field_tag :confirm_password %>
  <%= label_tag :current_password %>
  <%= password_field_tag :current_password %>
  <%= submit_tag "Edit" %>
<% end %>
